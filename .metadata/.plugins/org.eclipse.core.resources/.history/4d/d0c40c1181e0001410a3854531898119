
public class TrackerInfo {
	// Instance variables
	private String[] dataArray;
	
	public TrackerInfo(String msg) {
		dataArray = msg.split(" ");
	}
	
	/**
	 * Helper function to get the ith token as an int
	 * @param i
	 * @return
	 */
	private int intToken(int i) {
		return Integer.parseInt(dataArray[i]);
	}

